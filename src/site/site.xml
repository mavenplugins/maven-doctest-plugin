<project xmlns="http://maven.apache.org/DECORATION/1.1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd"
         name="${project.name}">
    <bannerLeft>
        <name>${project.name}</name>
        <href>${project.url}</href>
    </bannerLeft>
    <poweredBy>
        <logo href="https://buildhive.cloudbees.com/" img="http://www.cloudbees.com/sites/default/files/Button-Built-on-CB-1.png"/>
    </poweredBy>
    <body>
        <links>
            <item name="Apache" href="http://www.apache.org/"/>
            <item name="Maven 1.x" href="http://maven.apache.org/maven-1.x/"/>
            <item name="Maven 2" href="http://maven.apache.org/"/>
            <item name="${project.name}" href="${project.url}"/>
        </links>
        <menu ref="parent"/>
        <menu ref="modules"/>
        <menu ref="reports"/>
    </body>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.0</version>
    </skin>
</project>
